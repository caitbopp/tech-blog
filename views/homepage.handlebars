<div class="card">
{{#each posts as |post|}}
  <div class="card-header">
      <h5 class="post-header">{{post.title}}</h5>
      <span class="post-byline">Posted by {{username}} on {{date_created}}</span>
  <div class="card-body">
    <p class="card-text">{{post.content}}</p>
  </div>
  {{/each}}
</div>